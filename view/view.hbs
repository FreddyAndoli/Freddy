<!DOCTYPE html>
<html lang="en">
<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width,
			initial-scale=1, shrink-to-fit=no">
	<!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" >
    <link rel="stylesheet" href="../public/view.css">
	<title>Bootstrap | Tables</title>
	<style>
		h1 {
			color: rgb(0, 0, 0);
			text-align: center;
		}
		div {
			margin-top: 10px;
		}
	</style>
</head>
<body>
	<div class="container">
		<h1>INCIDENTS</h1>
		<div class="link-btn">
			<a href="./" style="color:black">BACK TO HOME PAGE</a> <br>
		</div>
		<!-- table-responsive -->
		<div class="table-responsive">
			<!-- table -->
			<table class="table">
				<thead>
					<tr>
						<th scope="col">TT_Number</th>
                        <th scope="col">ALARM_TIME</th>
                        <th scope="col">SITES</th>
                        <th scope="col">SITES_IMPACTED</th>
                        <th scope="col">ALARM</th>
                        <th scope="col">ESCALADE</th>
                        <th scope="col">ROOT_CAUSE</th>
                        <th scope="col">ACTION_DONE</th>
                        <th scope="col">CANCEL_TIME</th>
                        <th scope="col">DURATION</th>
                        <th scope="col">STATUS</th>
					
					</tr>
				</thead>
				<tbody>
                {{#data}}
                    <tr>
        <td>{{TT_Number}}</td>
        <td>{{ALARM_TIME}}</td>
        <td>{{SITES}}</td>
        <td>{{SITES_IMPACTED}}</td>
        <td>{{ALARM}}</td>
        <td>{{ESCALADE}}</td>
        <td>{{ROOT_CAUSE}}</td>
        <td>{{ACTION_DONE}}</td>
        <td>{{CANCEL_TIME}}</td>
        <td>{{DURATION}}</td>
        <td>{{STATUS}}</td>
    </tr>
    {{/data}}
    
                </tbody>
			</table>
		</div>
		
	</div>


</body>
<script>$(".modal").modal("show")</script>
</html>
